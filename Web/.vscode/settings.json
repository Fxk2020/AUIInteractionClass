{
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "html",
    "typescriptreact"
  ],

  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",

  "search.exclude": {
    ".git": true,
    ".eslintcache": true,
    "public": true,
    "node_modules": true,
    "package-lock.json": true,
    "*.{css,sass,scss}.d.ts": true
  }
}
